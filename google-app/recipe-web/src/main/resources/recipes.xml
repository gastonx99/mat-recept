	<recipe:recipe>
		<recipe:key>LAX_SPENATBADD</recipe:key>
		<recipe:name>Lax på spenatbädd</recipe:name>
		<recipe:type>FISH</recipe:type>
		<recipe:reference>Sida 23</recipe:reference>
		<recipe:ingredients>
			<recipe:ingredient>
				<recipe:name>Rökt lax</recipe:name>
				<recipe:amount>200</recipe:amount>
				<recipe:unit>GRAM</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Zucchini</recipe:name>
				<recipe:amount>2</recipe:amount>
				<recipe:unit>PCS</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Gul lök</recipe:name>
				<recipe:amount>1</recipe:amount>
				<recipe:unit>PCS</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Olivolja</recipe:name>
				<recipe:amount>1</recipe:amount>
				<recipe:unit>TSP</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Citron</recipe:name>
				<recipe:amount>1</recipe:amount>
				<recipe:unit>PCS</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Lätt creme fraiche</recipe:name>
				<recipe:amount>1</recipe:amount>
				<recipe:unit>DL</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Sockerärtor</recipe:name>
				<recipe:amount>100</recipe:amount>
				<recipe:unit>GRAM</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Svartpeppar</recipe:name>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Dill till dekor</recipe:name>
			</recipe:ingredient>
		</recipe:ingredients>
		<recipe:preparation>
			<recipe:time>20</recipe:time>
			<recipe:steps>
				<recipe:step>Börja koka upp lättsaltat vatten för zucchinin</recipe:step>
				<recipe:step>Gör långa strimlor av zucchinin med potatisskalare, hoppa över kärnhuset i mitten</recipe:step>
				<recipe:step>Strimla laxen</recipe:step>
				<recipe:step>Skala och finhacka löken</recipe:step>
				<recipe:step>Hetta upp olivolja och mjukstek löken utan att den tar färg</recipe:step>
				<recipe:step>Tillsätt laxen och låt den steka med i en halv minut</recipe:step>
				<recipe:step>Pressa över citron</recipe:step>
				<recipe:step>Peppra rejält</recipe:step>
				<recipe:step>Blanda i creme fraiche och sockerärtor</recipe:step>
				<recipe:step>Låt puttra i någon minut</recipe:step>
				<recipe:step>Häll såsen över zucchinin</recipe:step>
				<recipe:step>Lägg zucchinistrimlorna i en sil och sänk ned dem 10-15 sekunder i kokande lättsaltat vatten och sedan
					låta dem
					rinna av
				</recipe:step>
				<recipe:step>Dekorera med dillvippor och servera genast</recipe:step>
			</recipe:steps>
		</recipe:preparation>
	</recipe:recipe>
