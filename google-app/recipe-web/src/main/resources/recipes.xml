		<recipe:key>PIZZASALLAD</recipe:key>
		<recipe:name>Pizzasallad</recipe:name>
		<recipe:type>SIDE_ORDER_ROOT</recipe:type>
		<recipe:ingredients>
			<recipe:ingredient>
				<recipe:name>Vitkål</recipe:name>
				<recipe:amount>500</recipe:amount>
				<recipe:unit>GRAM</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Olivolja</recipe:name>
				<recipe:amount>2</recipe:amount>
				<recipe:unit>TSP</recipe:unit>
			</recipe:ingredient>
			<recipe:ingredient>
				<recipe:name>Vitvinsvinäger</recipe:name>
				<recipe:amount>1.5</recipe:amount>
				<recipe:unit>PCS</recipe:unit>
			</recipe:ingredient>
		</recipe:ingredients>
		<recipe:preparation>
			<recipe:time>20</recipe:time>
			<recipe:steps>
				<recipe:step>Strimla vitkålen</recipe:step>
				<recipe:step>Salta</recipe:step>
				<recipe:step>Knåda eller stöt (som i en mortel)</recipe:step>
				<recipe:step>Häll över olja, vinäger och peppar</recipe:step>
				<recipe:step>Rör om</recipe:step>
				<recipe:step>Låt stå i kylskåp, minst 2 timmar, helst halvt eller helt dygn</recipe:step>
			</recipe:steps>
		</recipe:preparation>
